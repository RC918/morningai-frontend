{
  "name": "morningai-frontend",
  "version": "2.2.0",
  "description": "Morning AI SaaS MVP Phase 2 Frontend - Design System & Component Library with i18n",
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "i18n:check": "node scripts/i18n-check.js",
    "i18n:diff": "node scripts/i18n-diff.mjs",
    "i18n:fill": "node scripts/i18n-fill.mjs",
    "i18n:lint:empty": "npx ts-node --transpile-only scripts/i18n/check-empty-keys.ts",
    "i18n:sync:empty": "npx ts-node --transpile-only scripts/i18n/fill-empty-with-base.ts",
    "i18n:lint": "npm-run-all -s i18n:lint:empty",
    "i18n:sync": "npm-run-all -s i18n:sync:empty",
    "ci:check": "npm run i18n:diff"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RC918/morningai-frontend.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "design-system"
  ],
  "author": "Morning AI Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RC918/morningai-frontend/issues"
  },
  "homepage": "https://github.com/RC918/morningai-frontend#readme",
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@sentry/nextjs": "^10.8.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/deepmerge": "^2.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "negotiator": "^1.0.0",
    "next": "14.2.32",
    "next-intl": "^3.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
